{
    "npr": {
        "rss_feed": "https://feeds.npr.org/1019/rss.xml",
        "base_url": "https://www.npr.org",
        "article_selector": "article",
        "headline_selector": "h1",
        "content_selector": "article",
        "timestamp_selector": "time",
        "timestamp_format": "%Y-%m-%dT%H:%M:%S%z",
        "headline": {"tag": "h1"},
        "content": {
            "container": {"tag": "div", "id": "storytext"},
            "paragraphs": {"tag": "p"}
        },
        "cleanup": [
            {"tag": "div", "class": "credit-caption"}
        ]
    },
    "ap_news": {
        "base_url": "https://apnews.com",
        "article_selector": "article",
        "headline_selector": "h1",
        "content_selector": "article",
        "timestamp_selector": "time",
        "timestamp_format": "%Y-%m-%dT%H:%M:%S%z",
        "headline": {"tag": "h1", "class": "Page-headline"},
        "content": {
            "container": {"tag": "div", "class": "RichTextStoryBody"},
            "paragraphs": {"tag": "p"}
        }
    },
    "bbc": {
        "rss_feed": "http://feeds.bbci.co.uk/news/technology/rss.xml",
        "base_url": "https://www.bbc.com",
        "article_selector": "article",
        "headline_selector": "h1",
        "content_selector": "article",
        "timestamp_selector": "time",
        "timestamp_format": "%Y-%m-%dT%H:%M:%S%z",
        "headline": {"tag": "h1"},
        "content": {
            "containers": {"tag": "div", "attrs": {"data-component": "text-block"}},
            "paragraphs": {"tag": "p"}
        },
        "cleanup": [
            {"tag": "div", "class": "credit-caption"}
        ],
        "additional_feeds": [
            "http://feeds.bbci.co.uk/news/business/rss.xml",
            "http://feeds.bbci.co.uk/news/technology/rss.xml"
        ]
    },
    "cnn": {
        "base_url": "https://www.cnn.com",
        "article_selector": "div.article__content",
        "headline_selector": "h1.headline__text",
        "content_selector": "div.article__content p.paragraph",
        "timestamp_selector": "div.timestamp",
        "timestamp_format": "%I:%M %p %Z, %a %B %d, %Y",
        "exclude_selectors": [
            "div.editor-note",
            "div.source",
            "div.ad-slot-dynamic",
            "div.related-content",
            "div.podcast-episode",
            "div.openweb-comment-counter",
            "div.bizdev-lending-tree"
        ]
    },
    "cnbc": {
        "rss_feed": "https://search.cnbc.com/rs/search/combinedcms/view.xml?partnerId=wrss01&id=19854910",
        "base_url": "https://www.cnbc.com",
        "article_selector": "item",
        "headline_selector": "title",
        "content_selector": "description",
        "timestamp_selector": "pubDate",
        "timestamp_format": "%a, %d %b %Y %H:%M:%S %z",
        "exclude_selectors": [
            "script",
            "style",
            "iframe",
            "nav",
            "header",
            "footer",
            "aside",
            ".advertisement",
            ".social-share",
            ".related-articles",
            ".newsletter-signup",
            ".comments-section"
        ],
        "additional_feeds": [
            "https://search.cnbc.com/rs/search/combinedcms/view.xml?partnerId=wrss01&id=10000664",
            "https://search.cnbc.com/rs/search/combinedcms/view.xml?partnerId=wrss01&id=20910258",
            "https://search.cnbc.com/rs/search/combinedcms/view.xml?partnerId=wrss01&id=15839135"
        ],
        "article_page": {
            "headline": {"tag": "h1", "class": "ArticleHeader-headline"},
            "timestamp": {"tag": "time", "attrs": {"data-testid": "published-timestamp"}},
            "content": {
                "container": {"tag": "div", "class": "group", "parent": {"tag": "div", "class": "ArticleBody-articleBody"}},
                "paragraphs": {"tag": "p"}
            },
            "cleanup": [
                {"tag": "div", "class": "BoxInline-container"},
                {"tag": "div", "class": "MidResponsive-midResponsiveContainer"},
                {"tag": "span", "class": "QuoteInBody-quoteNameContainer"},
                {"tag": "span", "class": "transition-fade-appear-done"},
                {"tag": "span", "class": "transition-fade-enter-done"},
                {"tag": "div", "class": "RelatedContent-relatedContent"}
            ]
        }
    }
}
